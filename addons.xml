<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.audio.googlemusic.exp"
       name="Google Music EXP"
       version="1.2"
       provider-name="ForeverGuest">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="xbmc.addon" version="12.0.0"/>
    <import addon="script.module.parsedom" version="2.5.1"/>
    <import addon="script.module.decorator" version="3.4.0"/>
    <import addon="script.module.requests" version="1.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>audio</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary lang="en">Experimental Google Music audio plugin</summary>
    <description lang="en">Plugin that lets you play music from Google Music.</description>
  </extension>
</addon>

<addon id="plugin.video.RobinHood-PPL-List"
       name="RobinHood-People List"
       version="2.0.1"
       provider-name="Husham.com">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.1"/>
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.beautifulsoup4" />
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.requests" />
    <import addon="script.module.httplib2" />
    <import addon="script.module.youtube.dl" optional="true"/>
    <import addon="plugin.video.youtube" />
    <import addon="script.module.urlresolver" optional="true"/>
    <import addon="script.module.simplejson" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">RobinHood People Lists - downloaded for free from:
http://www.husham.com</summary>
    <description lang="en">RobinHood-People List - The shadow crew finest addon
	The ShadowCrew Has been working round the clock to make this addon working great for all.
	http://www.husham.com
	P.S. Special Thanks to Shani for his Excellent work for LiveStream Pro add-on
</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.ronos" name="ronos Add-on repository" version="1.0.0" provider-name="ronos">
	<extension point="xbmc.addon.repository" name="ronos Add-on repository">
		<dir>
			<info compressed="false">https://raw.githubusercontent.com/ron23/ronos-repo/master/addons.xml</info>
			<checksum>https://raw.githubusercontent.com/ron23/ronos-repo/master/addons.xml.md5</checksum>
			<datadir zip="true">https://github.com/ron23/ronos-repo/tree/master/zips</datadir>
		</dir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Install Add-ons from ronos</summary>
		<description lang="en">Download and install add-ons from ronos repository.</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
