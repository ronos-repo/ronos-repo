<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.liveil.tv"
       name="LiveIL.TV"
       version="1.3.6"
       provider-name="liveil.tv">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
             provides="video"
             library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Israel TV made by LiveIL.tv</summary>
    <description>Israel Channel with Phone service braodcast 24/7 live and with timeshift</description>
    <platform>all</platform>
  </extension>
  <extension point="xbmc.addon.repository"
              name="Official XBMC.org Add-on Repository">
    <info compressed="true">http://xbmc.liveil.tv/svn/xbmc/trunk/addons.xml</info>
    <checksum>http://xbmc.liveil.tv/svn/xbmc/trunk/addons.xml.md5</checksum>
    <datadir zip="true">http://http://xbmc.liveil.tv/svn/xbmc/trunk</datadir>
  </extension>
</addon>
